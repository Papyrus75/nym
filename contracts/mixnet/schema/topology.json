{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Topology",
  "type": "object",
  "required": [
    "count",
    "nodes"
  ],
  "properties": {
    "count": {
      "type": "integer",
      "format": "int32"
    },
    "nodes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}
